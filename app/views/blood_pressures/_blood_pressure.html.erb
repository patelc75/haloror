<div id="event_event_<%= blood_pressure.id %>" style="padding-left:10px;cursor:pointer">
  <div style="float:left;width:350px">
    <%= UtilityHelper.format_datetime(blood_pressure.timestamp,blood_pressure.user) %>&nbsp;
  </div>
  <div style="float:left;width:150px">
    <%= blood_pressure.systolic + blood_pressure.diastolic %>
  </div>
  <div style="float:left;width:100px">
    <%= blood_pressure.diastolic %>
  </div>
  <div style="float:left;width:100px">
    <%=blood_pressure.map %>
  </div>
  <div style="float:left;width:100px">
    <%= blood_pressure.pulse %>
  </div>
  <div style="float:left;width:100px">
    <%= blood_pressure.battery %>
  </div>
</div>
<hr class="space" />
