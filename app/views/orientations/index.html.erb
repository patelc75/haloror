<h1>Listing orientations</h1>

<table>
  <tr>
  </tr>
  
<% for orientation in @orientations %>
  <tr>
    <td><%= link_to 'Show', orientation_path(orientation) %></td>
    <td><%= link_to 'Edit', edit_orientation_path(orientation) %></td>
    <td><%= link_to 'Destroy', orientation_path(orientation), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New orientation', new_orientation_path %>