<h1 id="audit_log">Audit log for user <%= @user.name %></h1>
<div class="span-8">
  <% @user.user_intakes.each do |user_intake| %>
  <div>User Intake <%= user_intake.id %>:
    <% user_intake.users.each do |user| %>
      <span><%= link_to "#{user.name}", user_audits_path( user.id) unless user.blank? %> </span>
    <% end %>
  </div>
  <% end %>
</div>
<hr />
<div><%= will_paginate @audits %></div>
<table class="bluegrey prepend-top">
  <tr>
    <th>Version</th>
    <th>From action</th>
    <th>IP address</th>
    <th>Referrer URL</th>
    <th>Created at</th>
    <th>By user</th>
  </tr>
  <%= render :partial => 'audit_row', :collection => @audits %>
</table>
<div><%= will_paginate @audits %></div>
