<h1>New raw data file</h1>


<%= error_messages_for :raw_data_file %>

<% form_for(:raw_data_file, :url => raw_data_files_path, :html => { :multipart => true }) do |form| %>

  <p>
    <label for="uploaded_data">Upload a file:</label>
    <%= form.file_field :uploaded_data %>
  </p>

  <p>
    <%= submit_tag "Create" %>
  </p>

<% end %>

<%= link_to 'Back', raw_data_files_path %>
