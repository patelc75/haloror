<% device = @device if @device%>
	<div class="row">
		<div class="col-25">&nbsp;</div>
		<div class="col-200">
			<h3>Users</h3>
		</div>
	</div>
	<% users = device.users
		 users.each do |user| %>
		<%=render :partial => 'reporting/table/user_row', :locals => {:device => device, :user => user, :toggle => false, :caregiver => false}%>
	<% end %>
