<div id="call_list-col">
	<div id="call_list-col-header">
		Account Settings: <span style="color:#000;font-size:.75em"><%= @call_list.login %>'s Caregivers</span>
	</div>
	<div class="content">
			<div style="float:left;width:70px">
				&nbsp;
			</div>
			<div style="float:left;width:125px">
				&nbsp;
			</div>
			<div style="float:left;width:175px">
				Name
			</div>
			<div style="float:left;width:150px">
				Status
			</div>
			<div style="float:left;width:75px">
				Call
			</div>
			<div style="float:left;width:75px">
				Email
			</div>
			<div style="float:left;width:125px">
				Text Message
			</div>
			<div style="float:left;width:50px">
				Delete
			</div>
			<div style="float:left;width:75px">
				
			</div>
			<p>&nbsp;</p>
      		<ul id="call_list">
			<% @call_list.call_orders.each do |call_order| %>
				<% @caregiver = call_order.caregiver %>
				
				<li id="item_<%= call_order.id %>" style="padding-left:10px;background:url('/images/call_list-item.gif') no-repeat;height:100px;" onMouseDown="this.style.backgroundImage = 'url(\'/images/call_list-item_drag.gif\')'" onMouseOver="this.style.backgroundImage = 'url(\'/images/call_list-item_drag.gif\')'" onMouseOut="this.style.backgroundImage = 'url(\'/images/call_list-item.gif\')'">
					<%= render :partial => "item", :locals => { :call_order => call_order } %>
				</li>
			<% end %>
			</ul>     

		<%= sortable_element 'call_list',
        :url => { :action => "sort" , :id => @call_list },

		 :tag => 'li'
        %>
	</div>
	<div id="blah"></div>
	<div id="call_list-col-footer"></div>
</div>